{
  "oxc.enable": true,
  "eslint.enable": false,
  "prettier.enable": false,
  "editor.formatOnSave": true,
  "editor.defaultFormatter": "oxc.oxc-vscode",
  "editor.codeActionsOnSave": {
    "source.fixAll.oxc": "explicit"
  },
  "files.associations": {
    ".oxlintrc.json": "jsonc"
  },
  "typescript.experimental.useTsgo": true,
  "cSpell.words": [
    "bumpp",
    "changelogithub",
    "codecov",
    "esbuild",
    "lcov",
    "oxfmt",
    "oxlint",
    "pnpm",
    "tsdown",
    "tsgo",
    "unrs",
    "vite",
    "vitepress",
    "vitest"
  ],
  "cSpell.allowCompoundWords": true
}
