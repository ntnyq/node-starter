{
  "name": "pkg-name",
  "type": "module",
  "version": "0.0.0",
  "packageManager": "pnpm@7.12.1",
  "description": "__TODO__",
  "keywords": [],
  "homepage": "https://github.com/ntnyq/pkg-name#readme",
  "bugs": {
    "url": "https://github.com/ntnyq/pkg-name/issues"
  },
  "license": "MIT",
  "author": {
    "name": "ntnyq",
    "email": "ntnyq13@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ntnyq/pkg-name.git"
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    },
    "./*": "./*"
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
    "lint": "eslint .",
    "prepublishOnly": "pnpm run build",
    "start": "tsx src/index.ts",
    "test": "vitest",
    "prepare": "husky install",
    "typecheck": "tsc --noEmit",
    "release": "run-s release:check release:publish",
    "release:check": "run-s lint typecheck clean build",
    "release:publish": "bumpp && pnpm publish"
  },
  "devDependencies": {
    "@ntnyq/eslint-config-typescript": "^1.0.4",
    "@types/node": "^18.7.18",
    "bumpp": "^8.2.1",
    "eslint": "^8.23.1",
    "fs-extra": "^10.1.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "npm-run-all": "^4.1.5",
    "picocolors": "^1.0.0",
    "pnpm": "^7.12.1",
    "tsx": "^3.9.0",
    "typescript": "^4.8.3",
    "unbuild": "^0.8.11",
    "vite": "^3.1.3",
    "vitest": "^0.23.4"
  },
  "lint-staged": {
    "*.{js,ts,json,md,yml,yaml}": "eslint --fix"
  },
  "eslintConfig": {
    "extends": "@ntnyq/typescript",
    "root": true
  }
}
