{
  "name": "node-starter",
  "version": "0.0.0",
  "description": "__TODO__",
  "keywords": [],
  "homepage": "https://github.com/ntnyq/node-starter#readme",
  "bugs": {
    "url": "https://github.com/ntnyq/node-starter/issues"
  },
  "license": "MIT",
  "author": {
    "name": "ntnyq",
    "email": "ntnyq13@gmail.com"
  },
  "repository": "ntnyq/node-starter",
  "files": [
    "dist"
  ],
  "type": "module",
  "sideEffects": false,
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown --watch",
    "format": "oxfmt",
    "format:check": "oxfmt --check",
    "lint": "oxlint",
    "prepare": "husky",
    "prepublishOnly": "pnpm run build",
    "release": "run-s release:check release:version",
    "release:check": "run-s format:check lint typecheck test",
    "release:version": "bumpp",
    "test": "vitest",
    "typecheck": "tsgo --noEmit"
  },
  "devDependencies": {
    "@ntnyq/tsconfig": "^3.1.0",
    "@types/node": "^25.3.0",
    "@typescript/native-preview": "^7.0.0-dev.20260224.1",
    "bumpp": "^10.4.1",
    "husky": "^9.1.7",
    "nano-staged": "^0.9.0",
    "npm-run-all2": "^8.0.4",
    "oxfmt": "^0.35.0",
    "oxlint": "^1.50.0",
    "tsdown": "^0.20.3",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18"
  },
  "nano-staged": {
    "*.{js,ts,mjs,tsx}": "oxlint --fix",
    "*": "oxfmt --no-error-on-unmatched-pattern"
  },
  "packageManager": "pnpm@10.30.2"
}
