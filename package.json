{
  "name": "starter-ts",
  "type": "module",
  "version": "0.0.0",
  "packageManager": "pnpm@8.11.0",
  "description": "__TODO__",
  "keywords": [],
  "license": "MIT",
  "author": {
    "name": "ntnyq",
    "email": "ntnyq13@gmail.com"
  },
  "homepage": "https://github.com/ntnyq/starter-ts#readme",
  "bugs": {
    "url": "https://github.com/ntnyq/starter-ts/issues"
  },
  "repository": "ntnyq/starter-ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "unbuild",
    "coverage": "vitest --coverage",
    "dev": "unbuild --stub",
    "lint": "eslint .",
    "prepare": "husky install",
    "prepublishOnly": "pnpm run build",
    "release": "run-s release:check release:publish",
    "release:check": "run-s lint typecheck test",
    "release:publish": "bumpp && pnpm publish",
    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@ntnyq/eslint-config": "^1.19.0",
    "@ntnyq/prettier-config": "^1.19.0",
    "@vitest/coverage-v8": "^1.0.0-beta.6",
    "bumpp": "^9.2.0",
    "eslint": "^8.54.0",
    "husky": "^8.0.3",
    "nano-staged": "^0.8.0",
    "npm-run-all": "^4.1.5",
    "pnpm": "^8.11.0",
    "prettier": "^3.1.0",
    "typescript": "^5.3.2",
    "unbuild": "2.0.0",
    "vitest": "^1.0.0-beta.6"
  },
  "engines": {
    "node": ">=18.12.0"
  },
  "prettier": "@ntnyq/prettier-config",
  "nano-staged": {
    "*.{js,ts,mjs,cjs,json,md,yml,yaml}": "eslint --fix"
  }
}
