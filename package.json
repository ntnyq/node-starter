{
  "name": "pkg-name",
  "version": "0.0.0",
  "packageManager": "pnpm@7.1.0",
  "description": "__TODO__",
  "keywords": [],
  "homepage": "https://github.com/ntnyq/pkg-name#readme",
  "bugs": {
    "url": "https://github.com/ntnyq/pkg-name/issues"
  },
  "license": "MIT",
  "author": "ntnyq <ntnyq13@gmail.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ntnyq/pkg-name.git"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    },
    "./*": "./*"
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "rimraf dist && unbuild",
    "dev": "unbuild --stub",
    "lint": "eslint .",
    "prepublishOnly": "pnpm run build",
    "start": "esno src/index.ts",
    "test": "vitest",
    "prepare": "husky install",
    "typecheck": "tsc --noEmit",
    "release": "run-s release:check release:version release:publish",
    "release:changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "release:check": "run-s lint typecheck clean build",
    "release:publish": "pnpm -r publish --tag next",
    "release:version": "bumpp package.json --execute=\"pnpm release:changelog\" --commit \"chore: release v%s\" --all --tag --push"
  },
  "devDependencies": {
    "@ntnyq/eslint-config-typescript": "^1.0.0",
    "@types/node": "^17.0.33",
    "bump": "^0.2.5",
    "conventional-changelog-cli": "^2.2.2",
    "eslint": "^8.15.0",
    "esno": "^0.15.0",
    "fs-extra": "^10.1.0",
    "husky": "^8.0.1",
    "lint-staged": "^12.4.1",
    "npm-run-all": "^4.1.5",
    "picocolors": "^1.0.0",
    "pnpm": "^7.1.0",
    "rimraf": "^3.0.2",
    "typescript": "^4.6.4",
    "unbuild": "^0.7.4",
    "vite": "^2.9.9",
    "vitest": "^0.12.6"
  },
  "lint-staged": {
    "*.{js,ts,json,md,yml,yaml}": "eslint --fix"
  },
  "eslintConfig": {
    "extends": "@ntnyq/typescript",
    "root": true
  }
}
